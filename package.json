{
  "name": "react-tailwind-ghpages-starter",
  "version": "1.0.0",
  "description": "Barebones starter for a React/Tailwind CSS PWA hosted via GitHub Pages. Uses GitHub Actions to build and deploy site.",
  "private": true,
  "engines": {
    "node": ">= 20.x",
    "npm": ">= 10.x"
  },
  "type": "module",
  "scripts": {
    "build": "npx vite build",
    "clean": "rm -rf ./build",
    "dev": "npx vite serve",
    "preview": "npx vite preview",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "url": "https://github.com/mikesprague/react-tailwind-ghpages-starter"
  },
  "keywords": [],
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/react-tailwind-ghpages-starter/issues"
  },
  "homepage": "https://mikesprague.github.io/react-tailwind-ghpages-starter",
  "dependencies": {
    "axios": "1.6.8",
    "daisyui": "4.9.0",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "he": "1.2.0",
    "nanoid": "5.0.6",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.3",
    "@fullhuman/postcss-purgecss": "6.0.0",
    "@tailwindcss/typography": "0.5.12",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "10.4.19",
    "cssnano": "6.1.2",
    "cz-git": "^1.9.1",
    "postcss": "8.4.38",
    "postcss-cli": "11.0.0",
    "sass": "1.72.0",
    "stylelint": "16.3.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "tailwindcss": "3.4.3",
    "vite": "5.2.7",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-pwa": "0.19.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "volta": {
    "node": "21.6.2",
    "npm": "10.5.0"
  }
}
